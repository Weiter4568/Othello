<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.othello.OthelloController">
   <children>
      <Label fx:id="welcomeLabel" layoutY="37.0" prefHeight="17.0" prefWidth="600.0" text="Welcome to Othello!">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <Label fx:id="modeLabel" layoutY="166.0" prefHeight="17.0" prefWidth="600.0" text="Please choose the mode" />
      <Button fx:id="robotAgainstMode" layoutX="102.0" layoutY="256.0" mnemonicParsing="false" text="人机模式" />
      <Button fx:id="personAgainstMode" layoutX="401.0" layoutY="256.0" mnemonicParsing="false" onAction="#onClickPVP" text="双人模式" />
   </children>
</AnchorPane>
